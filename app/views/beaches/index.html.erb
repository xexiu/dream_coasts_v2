<a href="" class="buttonScroll"></a>
  <div class="wrap-register">

  <% if user_signed_in? %>
  <ul>
<li><%= link_to '#', :class => 'btn-register' do %>
  <%= image_tag current_user.gravatar_url, :style => 'width: 70px; height: 70px; border-radius: 50%' %>
  <% end %>

</li>
</ul>
<div class="register">
  <%= link_to "Logout", destroy_user_session_path, :method => :delete %><br />
  <%= link_to "Profile", edit_user_registration_path %>
</div>
    <% else %>
      <ul>
        <li><%= link_to "Sign in", "#", :class => 'btn-register' %></li>
      </ul>

<div class="register">
  <h2 class="jumbotron">Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <div class="field">
    <%= f.label :username %><br />
    <%= f.text_field :login, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me, :checked => true %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", :class => 'btn btn-success' %>
  </div>
<% end %>
<%= render "devise/shared/links" %>

  <% end %>

</div>

</div>
<main id="main" class="cf">
  <span id="logo">
    <%= link_to root_path do %>
      Dream <i>Coasts</i></a></span>
    <% end %>
  <div id="breadcrumb">
    <ul class="crumbs">
      <li class="first">
        <%= link_to root_path, {:style => 'z-index: 9;'} do %><span></span>Home<% end %>
      </li>
      <li>
        <%= link_to beaches_path, {:style => 'z-index: 8;'} do %>
        Browse All
        <% end %>
        </li>
    </ul>
  </div>

  <!-- <li><%= link_to 'Admin',        new_admin_session_path %></li><br /> -->

  <aside id="menu_items">
  </aside>
  <section id="important" class="cf">
    <h1>Most Voted Coasts</h1>
  </section>
</main>
